    document.addEventListener("DOMContentLoaded", function () {
        var carousel = document.getElementById("ogi-test4");
        
        var prevArrow = document.createElement("a");
        prevArrow.classList.add("carousel-control-prev");
        prevArrow.setAttribute("href", "#ogi-test4");
        prevArrow.setAttribute("data-slide", "prev");
        
        var prevIcon = document.createElement("span");
        prevIcon.classList.add("carousel-control-prev-icon");
        prevArrow.appendChild(prevIcon);
        
        var nextArrow = document.createElement("a");
        nextArrow.classList.add("carousel-control-next");
        nextArrow.setAttribute("href", "#ogi-test4");
        nextArrow.setAttribute("data-slide", "next");
        
        var nextIcon = document.createElement("span");
        nextIcon.classList.add("carousel-control-next-icon");
        nextArrow.appendChild(nextIcon);
        
        carousel.appendChild(prevArrow);
        carousel.appendChild(nextArrow);
