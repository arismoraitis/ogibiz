BANNER VIDEO (YOUTUBE)

    <div id="ogi-banner-vid" class="video-background1">
        <div class="video-foreground1">
        <div id="player">&nbsp;</div>
        <!-- YouTube player container --></div>
        <div id="white-box-vid" class="col-12 col-xl-6 col-lg-8 col-md-12">
        <div class="bg-white opacity-9 border-radius-6px p-10 lg-p-10" style="text-align: center;">
        <h3 class="resp-title-6 text-base-color fw-700 ls-minus-1px mb-0" style="text-align: center;">Lorem ipsum is simply dummy of the printing typesetting industry lorem ipsum has the industry standard dummy text ever since lorem text.<br />&nbsp;</h3>
        <a class="text-white btn btn-large btn-rounded bg-base-color btn-hover-animation-switch" href="#"><span class="resp-btn btn-text">Get in Touch</span>&nbsp;<span class="btn-icon"><span class="resp-btn text-white fa-solid fa-arrow-right">&nbsp;</span></span></a></div>
        </div>
        </div>
        <!-- YouTube API Script -->
        <script>// <![CDATA[
        var tag = document.createElement('script');
          tag.src = "https://www.youtube.com/iframe_api";
          var firstScriptTag = document.getElementsByTagName('script')[0];
          firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);
        
          var player;
          function onYouTubeIframeAPIReady() {
            player = new YT.Player('player', {
              height: '150',
              width: '300',
              videoId: 'nnIhUI2VFT4', // Μόνο το ID, χωρίς "?si=..."
              playerVars: {
                autoplay: 1, // Αυτόματη εκκίνηση
                mute: 1, // Σίγαση για να παίξει αυτόματα
                controls: 0, // Απόκρυψη controls
                modestbranding: 1, // Αφαίρεση YouTube logo
                showinfo: 0, 
                loop: 1, // Ενεργοποίηση loop
                playlist: 'nnIhUI2VFT4', // Για να λειτουργεί το loop
                disablekb: 1, // Απενεργοποίηση keyboard shortcuts
                fs: 0, // Απόκρυψη fullscreen button
                rel: 0 // Απόκρυψη σχετικών βίντεο
              },
              events: {
                onReady: function(event) {
                  event.target.playVideo(); // Αυτόματη αναπαραγωγή όταν φορτώσει
                }
              }
            });
          }
        // ]]></script>
        <!-- Ρύθμιση ύψους του box σε σχέση με το iframe -->
        <script>// <![CDATA[
        function adjustBoxHeight() {
          const iframe = document.getElementById('player');
          const box = document.getElementById('white-box-vid');
          if (!iframe || !box) return;
        
          // Μόνο για desktop:
          if (window.innerWidth > 768) {
            const iframeHeight = iframe.offsetHeight;
            box.style.height = (iframeHeight * 0.95) + 'px';
          } else {
            // Σε mobile, άστο auto
            box.style.height = 'auto';
          }
        }
        // ]]></script>


