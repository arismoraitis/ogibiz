    <script>// <![CDATA[
    document.getElementById('cont_form').addEventListener('submit', function(e) {
          e.preventDefault(); // Αποφυγή παραδοσιακής υποβολής της φόρμας
    
          var form = this;
          var responseDiv = document.getElementById('response-message');
    
          // Έλεγχος εγκυρότητας (με χρήση HTML5 validation)
          if (!form.checkValidity()) {
            responseDiv.innerHTML = "<p style='color:red;'>Παρακαλώ συμπληρώστε όλα τα απαιτούμενα πεδία.</p>";
            return;
          }
    
          // Δημιουργία αντικειμένου FormData για αποστολή των δεδομένων
          var formData = new FormData(form);
    
          // Αντικαταστήστε το παρακάτω URL με το δικό σας Google Apps Script URL
          var scriptURL = "https://script.google.com/macros/s/AKfycbyugTrAxzuyv7RsTGrONxdy-IHfmbFlp7iaKSSg_WactQi8e2n5tRuhd6q-AGkvtjid/exec";
    
          // Αποστολή των δεδομένων με fetch
          fetch(scriptURL, {
            method: 'POST',
            mode: 'no-cors',  // Χρήση no-cors για αποφυγή CORS προβλημάτων
            body: formData
          })
          .then(function() {
            responseDiv.innerHTML = "<p style='color:green;'>Η φόρμα εστάλη επιτυχώς.</p>";
            form.reset();  // Επαναφορά φόρμας μετά την επιτυχή αποστολή
          })
          .catch(function(error) {
            responseDiv.innerHTML = "<p style='color:red;'>Παρουσιάστηκε σφάλμα. Παρακαλώ προσπαθήστε ξανά.</p>";
            console.error('Error!', error.message);
          });
        });
    // ]]></script>



